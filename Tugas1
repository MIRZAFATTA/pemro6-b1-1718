public class DataTypeExample { 
 public static void main(String[] args) { 
 String text = "text example"; 
 int bilangan = 8; 
 double bilanganPecahan = 2.5; 
System.out.println("Text: " + text); 
 // Tugas tamplkan bilangan dan bilangan pecahan 
System.out.println("bilangannya adalah guys" + bilangan); 
System.out.println("bilangan pecahannya adalah guys" + bilanganPecahan); 
} 
} 
